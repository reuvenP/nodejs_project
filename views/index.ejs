    <script>
        function calcHashedLogin()
        {
            var username = $("#username").val();
            var password = $("#password").val();
            var random = $("#random").val();
            var hash = CryptoJS.SHA1(username + ':' + password + ':' + random);
            var hash_Base64 = hash.toString(CryptoJS.enc.Base64);
            $("#hashedLogin").val(hash_Base64);
            return true;
        }
    </script>
